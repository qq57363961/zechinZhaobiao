/* pages/detail/detail.wxss */
/* 背景颜色 */
page {
  background: #f3f3f3;
}
/* 页面触摸水波纹效果 */

.ripple {
    background-color:aquamarine;
    border-radius: 100%;
    height:5rpx;
    width:5rpx;
    margin-top: -90px;
    position: absolute;
    overflow: hidden;
    z-index: 10;
}
@-webkit-keyframes ripple {
    100% {
    webkit-transform: scale(50);
    transform: scale(50);
    background-color: transparent;
    }
}
.container{
  width: 100%;
  height: 100%;
}
